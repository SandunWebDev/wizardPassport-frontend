{
	"printWidth": 80,
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"trailingComma": "all",
	"bracketSpacing": true,
	"arrowParens": "always",
	"requirePragma": false,
	"proseWrap": "preserve",
	"endOfLine": "lf",
	"bracketSameLine": true,
	"jsxSingleQuote": true
}
