{
	"name": "wizard-passport-frontend",
	"version": "0.1.0",
	"private": false,
	"scripts": {
		"start": "react-scripts start",
		"start-craco": "craco start",
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"test": "react-scripts test",
		"test-coverage": "react-scripts test --coverage",
		"test-ci": "react-scripts test --coverage --watchAll=false --passWithNoTests",
		"lint-prettier": "prettier --check .",
		"lint-prettier-fix": "prettier --write .",
		"lint-eslint": "eslint . --quiet ",
		"lint-eslint-fix": "eslint . --fix --quiet",
		"lint-stylelint": "stylelint **/*.{css,scss,sass,less}",
		"lint-stylelint-fix": "stylelint **/*.{css,scss,sass,less} --fix",
		"lint-all": "npm run lint-prettier && npm run lint-eslint && npm run lint-stylelint",
		"lint-all-fix": "npm run lint-prettier-fix && npm run lint-eslint-fix && npm run lint-stylelint-fix",
		"manual-tscheck": "tsc -p ./tsconfig.json",
		"full-code-quality-check": "npm run lint-all && npm run manual-tscheck && npm run test-ci",
		"prepare": "husky install"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx}"
		],
		"coverageThreshold": {
			"global": {
				"branches": 0,
				"functions": 0,
				"lines": 0,
				"statements": -1000
			}
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@chakra-ui/icons": "^1.1.1",
		"@chakra-ui/react": "^1.7.4",
		"@chakra-ui/theme-tools": "^1.3.1",
		"@emotion/react": "^11.7.1",
		"@emotion/styled": "^11.6.0",
		"@fontsource/comforter-brush": "^4.5.1",
		"@fontsource/fugaz-one": "^4.5.0",
		"@fontsource/saira-condensed": "^4.5.0",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.4.0",
		"@types/node": "^16.11.17",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"axios": "^0.24.0",
		"framer-motion": "^5.5.6",
		"history": "^5.2.0",
		"loglevel": "^1.8.0",
		"normalize.css": "^8.0.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-draggable": "^4.4.4",
		"react-error-boundary": "^3.1.4",
		"react-hot-toast": "^2.2.0",
		"react-icons": "^4.3.1",
		"react-location": "^3.3.0",
		"react-location-jsurl": "^3.3.0",
		"react-location-rank-routes": "^3.3.0",
		"react-scripts": "5.0.0",
		"typescript": "^4.5.4",
		"waait": "^1.0.5",
		"web-vitals": "^2.1.2"
	},
	"devDependencies": {
		"@craco/craco": "^6.4.3",
		"@types/react-resizable": "^1.7.4",
		"@typescript-eslint/eslint-plugin": "^5.9.0",
		"@typescript-eslint/parser": "^5.9.0",
		"eslint": "^8.6.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^16.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-react-redux": "^3.3.4",
		"eslint-plugin-sonarjs": "^0.11.0",
		"eslint-plugin-unicorn": "^40.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.1.7",
		"prettier": "^2.5.1",
		"react-json-view": "^1.21.3",
		"react-location-devtools": "^3.3.0",
		"stylelint": "^14.2.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-standard": "^24.0.0",
		"stylelint-order": "^5.0.0"
	}
}
